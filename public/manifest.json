{
  "manifest_version": 2,

  "name": "Pokemon Showdown Extension",
  "description": "Enable Showdown Extension",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": ["activeTab"],
  "optional_permissions": ["<all_urls>"]
}
